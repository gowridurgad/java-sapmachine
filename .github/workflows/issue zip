
name: testingcache

on:
  workflow_dispatch:

jobs:
  build:
    runs-on: selfhosted

    steps:
    - uses: actions/checkout@v4

    - name: Install poetry
      run: pipx install poetry

     - name: Setup python
        uses: actions/setup-python@v5
        with:
          python-version: '3.6.x - 3.11.x'

    - name: Install dependencies
      run: poetry install 
